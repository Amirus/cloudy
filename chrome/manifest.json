{
  "name": "Cloudy",
  "version": "0.1.5",
  "description": "Attach to Gmail from the cloud!",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" }, 
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "http://mail.google.com/*"],
      "js": ["js/lib/bootstrap.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://mail.google.com/*",
    "http://mail.google.com/",
    "https://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/",
    "https://*/"
  ],
  "manifest_version" : 2,
  "web_accessible_resources" : [ 
    "js/constants.js", 
    "js/utils.js", 
    "js/model.js", 
    "js/view.js", 
    "js/controller.js", 
    "js/main.js",
    "js/decode-worker.js", 
    "js/resources.js",
    "js/lib/bootstrap.js", 
    "js/lib/gmailr.js", 
    "js/lib/init.js", 
    "js/lib/jquery.ba-bbq.js", 
    "js/lib/lab.js", 
    "images/loading-ring.gif", 
    "images/checkmark.png", 
    "images/cloudIcon.png", 
    "images/error.png",
    "templates/downloaddiv.html", 
    "templates/customrow.html", 
    "js/main.css"
  ]
}
